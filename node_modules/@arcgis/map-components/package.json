{
  "name": "@arcgis/map-components",
  "description": "ArcGIS Map Components",
  "homepage": "https://developers.arcgis.com/javascript/latest/",
  "version": "4.30.0-next.37",
  "module": "dist/components/index.js",
  "main": "dist/index.cjs.js",
  "types": "dist/types/index.d.ts",
  "keywords": [
    "ArcGIS",
    "javascript",
    "map",
    "3D",
    "2D",
    "visualization",
    "analytics",
    "spatial",
    "data-driven",
    "gis",
    "components",
    "web-components",
    "Esri"
  ],
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "start": "yarn build:dev --watch --serve",
    "start:storybook": "storybook dev -c .storybook/internal -p 6007 --quiet",
    "start:external-storybook": "storybook dev -c .storybook/external dev -p 6017 --quiet",
    "watch": "yarn build:dev --watch",
    "build": "yarn pre && yarn build:components",
    "build:dev": "yarn pre:dev && yarn build:components:dev",
    "build:storybook": "storybook build -c .storybook/internal --quiet",
    "build:external-storybook": "storybook build -c .storybook/external -o storybook-static-external --quiet",
    "build:components": "stencil build",
    "build:components:dev": "stencil build --dev",
    "build:composite-storybook": "yarn awc copy-folder storybook-static-external ../docs/storybook-static-external/map-components",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "clean": "rimraf ./node_modules ./storybook-static ./storybook-static-external ./turbo ./dist ./www",
    "pre": "rimraf ./dist ./www && yarn awc build-t9n-types",
    "pre:dev": "yarn pre && yarn awc create-mock-doc"
  },
  "dependencies": {
    "@stencil/core": "4.9.0"
  },
  "devDependencies": {
    "@arcgis/components-controllers": "4.30.0-next.37",
    "@arcgis/components-plugins": "4.30.0-next.37",
    "@arcgis/components-utils": "4.30.0-next.37",
    "@arcgis/core": ">=4.30.0-next <4.31",
    "@arcgis/core-adapter": "4.30.0-next.37",
    "@arcgis/storybook-utils": "4.30.0-next.37",
    "@arcgis/typescript-config": "4.30.0-next.37",
    "@esri/calcite-components": "^2.7.1",
    "@stencil/angular-output-target": "0.8.4",
    "@stencil/react-output-target": "0.5.3",
    "@types/jest": "^29.5.0",
    "chromatic": "^11.3.0",
    "eslint": "^8.55.0",
    "jest": "^29.5.0",
    "jest-cli": "^29.5.0",
    "puppeteer": "^21.9.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.25.0",
    "rollup-plugin-gzip": "^3.1.0",
    "storybook": "^8.0.5",
    "typescript": "~5.3.0",
    "zlib": "^1.0.5"
  },
  "peerDependencies": {
    "@arcgis/core": ">=4.30.0-next <4.31",
    "@esri/calcite-components": ">=2.2.0 <3.0.0"
  },
  "gitHead": "c9918bf641cc3aba25aa9e4dfcf3c87fe1b59a93"
}
